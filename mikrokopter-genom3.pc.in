# pkg-config file for mikrokopter interface and codels library
#
prefix=@prefix@
exec_prefix=@exec_prefix@
libdir=@libdir@
includedir=@includedir@
datarootdir=@datarootdir@
idldir=@datadir@/idl

Name: mikrokopter-genom3
Description: mikrokopter interface and codels library
Version: @PACKAGE_VERSION@
Requires: openrobots2-idl >= 2.0, genom3 >= 2.99.30, eigen3
Cflags: -I${includedir} -I${idldir}
Libs: -L${libdir} -lmikrokopter_codels
